package pp;

public class student extends person{
	int id;
	public student(String name,int age,int id) {
		super(name,age);
		this.id=id;
		
	}
	public int getId() {
		return id;
	}
	public void setId(int id)
	{
		this.id=id;
	}
	public void print() {
		super.print();
		System.out.println("id="+id);

	}
	
	
}
