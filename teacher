package pp;

public class teacher extends person{
	float salary;
	public teacher(String name, int id , float salary ) {
		super(name,id);
		this.salary=salary;
	}
	public float getSalary() {
		return salary;
	}
	public void setSalary(float salary) {
		this.salary=salary;
	}
	public void print() {
		super.print();
		System.out.println("salary="+salary);

	}
}
